{
  "debug.toolBarLocation": "docked",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "editor.bracketPairColorization.enabled": true,
  "editor.smoothScrolling": true,
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": "off",
  "editor.cursorSurroundingLines": 1,
  "editor.fontLigatures": true,
  "editor.guides.bracketPairs": true,
  "editor.renderWhitespace": "trailing",
  "editor.rulers": [80],
  "editor.scrollPredominantAxis": false,
  "editor.suggest.preview": true,
  "editor.tabSize": 2,
  "problems.showCurrentInStatus": true,
  "explorer.openEditors.visible": 10,
  "files.hotExit": "off",
  "files.trimTrailingWhitespace": true,
  "files.enableTrash": false,
  "search.showLineNumbers": true,
  "search.smartCase": true,
  "window.autoDetectColorScheme": true,
  "window.openFoldersInNewWindow": "on",
  "window.commandCenter": false,
  "workbench.activityBar.location": "top",
  "workbench.colorTheme": "Monokai",
  "workbench.enableExperiments": false,
  "workbench.list.smoothScrolling": true,
  "workbench.quickOpen.closeOnFocusLost": false,
  "workbench.quickOpen.preserveInput": true,
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.decorations.badges": true,
  "workbench.editor.decorations.colors": true,
  "workbench.editor.enablePreview": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.labelFormat": "short",
  "workbench.layoutControl.enabled": false,
  "workbench.navigationControl.enabled": false,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "workbench.tree.renderIndentGuides": "always",
  "update.mode": "none",
  "update.showReleaseNotes": false,
  "security.workspace.trust.enabled": false,
  "code-runner.runInTerminal": true,
  "code-runner.saveFileBeforeRun": true,
  "dart.allowAnalytics": false,
  "dart.debugExternalPackageLibraries": true,
  "dart.debugSdkLibraries": true,
  "dart.flutterHotReloadOnSave": "never",
  "dart.flutterHotRestartOnSave": false,
  "dart.previewFlutterUiGuides": true,
  "dart.promptToGetPackages": false,
  "dart.checkForSdkUpdates": false,
  "dart.useLsp": true,
  "dart.automaticCommentSlashes": "all",
  "github.gitAuthentication": false,
  "python.experiments.enabled": false,
  "redhat.telemetry.enabled": false,
  "vim.hlsearch": true,
  "vim.mouseSelectionGoesIntoVisualMode": false,
  "vim.normalModeKeyBindings": [
      { "before": ["Z", "Q"], "commands": [":q!"] },
      { "before": ["Z", "Z"], "commands": [":x"] }
  ],
  "vim.useSystemClipboard": true
}
